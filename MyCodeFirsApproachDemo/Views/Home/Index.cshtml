﻿@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @{
        if (@TempData["Message"] != null)
        {
            <div class="alert alert-danger alert-dismissible fade show">
                <button type="button" class="fa fa-apple" data-dismiss="alert">&times;</button>
                @TempData["Message"]
            </div>
        }
    }
    <h1 class="display-4">Welcome Admin @ViewData["MessageInfo"]</h1>

    @{
        try
        {
            int a = 0;
            int result = 10 / a;
        }
        catch(DivideByZeroException ex)
        {
            <p>Error: @ex.Message</p>
        }
        finally
        {
            <p> This execute is compiler</p>
        }
    }

</div>